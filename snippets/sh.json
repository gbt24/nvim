{
  "notification": {
    "prefix": "noti",
    "body": [
      "osascript -e 'display notification \"${1:body}\" with title \"${2:title}\"'"
    ]
  },
  "shebang": {
    "prefix": "shebang",
    "body": [
      "#!/bin/bash",
      "",
      "${1}"
    ]
  },
  "arg1": {
    "prefix": "arg1",
    "body": [
      "if [ ! \\$# -gt 0 ]; then",
      "\techo \"No argument provided\"",
      "\texit 1",
      "fi",
      "",
      "${1}"
    ]
  }
}
